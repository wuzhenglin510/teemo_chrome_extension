{
  "name": "Teemo",
  "version": "1.0",
  "description": "smart ui test recorder and manager",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "page/popup/popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "content_security_policy":"script-src 'self' 'unsafe-eval';",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequestBlocking",
    "storage",
    "privacy",
    "tabs",
    "webNavigation"
  ]
}